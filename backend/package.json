{
  "name": "edtech-platform-backend",
  "version": "1.0.0",
  "description": "Advanced EdTech Platform Backend with AI-driven personalized learning, adaptive assessment, and gamification",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "npm ci --only=production",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint . --ext .js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@google/generative-ai": "^0.20.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "firebase": "^10.8.0",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "edtech",
    "ai",
    "personalized-learning",
    "microlearning",
    "adaptive-assessment",
    "gamification",
    "firebase",
    "nodejs",
    "express"
  ],
  "author": "EdTech Platform Team",
  "license": "MIT",
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  }
